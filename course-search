                                    </font></div><font face="Tahoma">
            </font></div><font face="Tahoma">
                        <div class="row">
                <div class="col-lg-12 js-ws-results" id="search-results">
                    <div class="ws-results">
                        <h2 class="courses__heading js-courses-title" style="display: none;"></h2>
                        <div class="js-course-intro"></div>
                        <div class="js-course-count"></div>
                        <div class="js-results" style=""></div>
                        <div class="loader js-loader-results" style="display: none;"><span class="visually-hidden">Loading...</span></div>
                    </div>
                </div>
            </div>
        </font></div><font face="Tahoma">
    </font></section><font face="Tahoma">
</font></div><font face="Tahoma">

<script>
    
    var searchResults = function(searchterm, subjectsFilter, modeofstudyFilter, levelFilter) {
        
        // if params not set
        searchterm = (typeof searchterm !== 'undefined') ?  searchterm : "";
        subjectsFilter = (typeof subjectsFilter !== 'undefined') ?  subjectsFilter : [];
        modeofstudyFilter = (typeof modeofstudyFilter !== 'undefined') ?  modeofstudyFilter : [];
        levelFilter = (typeof levelFilter !== 'undefined') ?  levelFilter : [];
    
        var $results = jQuery('.js-results');
        var $loading = jQuery('.js-loader-results');
        var $allCourses = jQuery('.js-all-courses');
        var $allCoursesLink = jQuery('.js-all-courses-link');
        var $allCheckboxes = jQuery('input[type="checkbox"]');
        var $coursesTitle = jQuery('.js-courses-title');
        var $resetSearchLink = jQuery('.js-reset-search');
        var $clearFiltersLink = jQuery('.js-clear-filters');
        var $subjectBlocks = jQuery('.js-subject-block').hide();
    
        var sendData = {
            searchterm: searchterm,
            subjectsFilter: subjectsFilter,
            modeofstudyFilter: modeofstudyFilter,
            levelFilter: levelFilter
        };
        console.log('sendData:');
        console.log(sendData);
    
        $allCoursesLink.attr('aria-expanded','false');
        $subjectBlocks.hide();
        $results.hide();
        $loading.show();
    
        jQuery.getJSON('/udcourses/fullcoursesearch', sendData).done(function(data) {
        
            console.log('returnData:');
            console.log(data);
            var markup = '';
            var courseCount = '';
            
            if(data.results.length){
                courseCount = "Displaying " + data.results.length + " courses found.";
                jQuery('.js-course-intro').html('<p class="courses__count-intro">The following courses match your search criteria. Click the course cards to see full details and to apply or enquire.</p>');
            } else {
                courseCount = "No courses found.";
                jQuery('.js-course-intro').html('');
            }
            jQuery('.js-course-count').html('<p class="courses__count-text">' + courseCount + '</p>');
            
    
            if(data.isSearch || data.filtersOn){ // if either filters or search are selected
                if(data.results.length) {
        
                    $allCourses.hide(); //remove initial results
                    // enable checkboxes
                    $allCheckboxes.removeAttr("disabled");
                    //change background
                    jQuery('.search-results').addClass('results-active');
        
                    //courses
                    for (i in data.results) {
                        markup += '<a class="courses__block" href="'+ data.results[i].url +'">';
                        markup += '<h2 class="courses__block-heading">'+ data.results[i].name +'</h2>';
                        markup += '<div class="courses__block-intro"><p>'+ data.results[i].intro +'</p></div>';
                        markup += '<div class="courses__date-block">';
                        
                        if(data.results[i].startdate != null){
                            markup += '<div class="courses__date">';
                            markup += '<p class="courses__date-label"><svg class="courses_calendar-icon" viewBox="0 0 14 16"><use xlink:href="#calendar"></use></svg>Start</p>';
                            markup += '<p class="courses__date-text">'+ data.results[i].startdateFormat +'</p>';
                            markup += '</div>';
                        }
                        if(data.results[i].enddate != null) {
                            markup += '<div class="courses__date">';
                            markup += '<p class="courses__date-label"><svg class="courses_calendar-icon" viewBox="0 0 14 16"><use xlink:href="#calendar"></use></svg>End</p>';
                            markup += '<p class="courses__date-text">' + data.results[i].enddateFormat + '</p>';
                            markup += '</div>';
                        }
    
                        markup += '</div>';
                        if(data.results[i].levelColour != null) {
                            markup += '<div class="courses__block-color" style="background-color: ' + data.results[i].levelColour + '"></div>';
                        } else {
                            markup += '<div class="courses__block-color"></div>';
                        }
                        markup += '</a>';
                    }
        
                    
                    if(data.count > 0){
                        if (data.isSearch) {
                            $coursesTitle.show();
                            $coursesTitle.html('It\'s a match for "'+ data.term +'"!');
                        } else{
                            $coursesTitle.show();
                            $coursesTitle.html('It\'s a match!');
                        }
                    } else {
                        $coursesTitle.show();
                        $coursesTitle.html('Your search provided no results');
                    }
        
                    //reset search link
                    if (data.isSearch){
                        $resetSearchLink.show();
                    }
        
                } else if (data.isSearch) {
                    $coursesTitle.show();
                    $coursesTitle.html('Your search for "'+ data.term +'" found no results');
                    $allCourses.hide(); //remove initial results
                    // enable checkboxes
                    $allCheckboxes.removeAttr("disabled");
                    //reset search link
                    if (data.isSearch){
                        $resetSearchLink.show();
                    }
                } else {
                    $coursesTitle.show();
                    $coursesTitle.html('Your search provided no results');
                    //remove initial results
                    $allCourses.hide();
                    // enable checkboxes
                    $allCheckboxes.removeAttr("disabled");
                    //reset search link
                    if (data.isSearch){
                        $resetSearchLink.show();
                    }
                }
            } else {
                $allCourses.hide();
                $subjectBlocks.show();
                $coursesTitle.html('');
                $coursesTitle.hide(); // for accessibility - no empty headings
                // enable checkboxes
                $allCheckboxes.removeAttr("disabled");
                //clear markup
                jQuery('.js-course-intro').html('');
                jQuery('.js-course-count').html('');
                //change background
                jQuery('.search-results').removeClass('results-active');
                
            }
    
            //clear filters link
            if (data.filtersOn) {
                $clearFiltersLink.show();
            } else {
                $clearFiltersLink.hide();
            }
    
            $loading.hide();
            $results.html(markup);
            $results.fadeIn('fast');
            
    
        }).fail(function(xhr, status, error) {
            console.log(error);
        });
    };
    
    
    document.addEventListener('DOMContentLoaded', function() {
    
        var $allCheckboxes = jQuery('input[type="checkbox"]');
        var $searchInput = jQuery('#searchterm-cs');
        var searchterm = "";
        var modeofstudyFilter = [];
        var subjectsFilter = [];
        var levelFilter = [];
    
         //local storage saves results for if going back to search results #}
    
            // search term
            if(localStorage.getItem("searchTerm") !== null){
                var searchTermLocal = localStorage.getItem("searchTerm");
                $searchInput.val(searchTermLocal);
            
            } else {
                var searchTermLocal = "";
            }
        
            // subjectsFilter
            if(localStorage.getItem("subjectsFilter") !== null && localStorage.getItem("subjectsFilter") !== ""){
                var subjectsFilterLocal = localStorage.getItem("subjectsFilter");
                var subjectsFilterLocalArr = subjectsFilterLocal.split(',');
            
                //mark checkboxes as checked
                for (i = 0; i < subjectsFilterLocalArr.length; i++) {
                    jQuery('.js-subjects input:checkbox[value="' + subjectsFilterLocalArr[i] + '"]').prop('checked', true);
                }
            
                //open accordion
                jQuery('.js-subjects a.js-filters-trigger').addClass('expanded');
                jQuery('.js-subjects .search-filters__expandable').css('display','block');
            } else {
                var subjectsFilterLocalArr = [];
            }
        
            // modeofstudyFilter
            if(localStorage.getItem("modeofstudyFilter") !== null && localStorage.getItem("modeofstudyFilter") !== ""){
                var modeofstudyFilterLocal = localStorage.getItem("modeofstudyFilter");
                var modeofstudyFilterLocalArr = modeofstudyFilterLocal.split(',');
            
                //mark checkboxes as checked
                for (i = 0; i < modeofstudyFilterLocalArr.length; i++) {
                    jQuery('.js-moa input:checkbox[value="' + modeofstudyFilterLocalArr[i] + '"]').prop('checked', true);
                }
            
                //open accordion
                jQuery('.js-moa a.js-filters-trigger').addClass('expanded');
                jQuery('.js-moa .search-filters__expandable').css('display','block');
            } else {
                var modeofstudyFilterLocalArr = [];
            }
        
            // levelFilter
            if(localStorage.getItem("levelFilter") !== null && localStorage.getItem("levelFilter") !== ""){
                var levelFilterLocal = localStorage.getItem("levelFilter");
                var levelFilterLocalArr = levelFilterLocal.split(',');
            
                //mark checkboxes as checked
                for (i = 0; i < levelFilterLocalArr.length; i++) {
                    jQuery('.js-level input:checkbox[value="' + levelFilterLocalArr[i] + '"]').prop('checked', true);
                }
            
                //open accordion
                jQuery('.js-level a.js-filters-trigger').addClass('expanded');
                jQuery('.js-level .search-filters__expandable').css('display','block');
            } else {
                var levelFilterLocalArr = [];
            }
        
            searchResults(searchTermLocal, subjectsFilterLocalArr, modeofstudyFilterLocalArr, levelFilterLocalArr);
        
    
            
        //on form submit
        jQuery("#searchterm-form").submit(function(e) {
    
            e.preventDefault();
            searchterm = $searchInput.val();
    
            //remove filters for new search
            modeofstudyFilter = [];
            subjectsFilter = [];
            levelFilter = [];
            $allCheckboxes.prop('checked', false);
            searchResults(searchterm, subjectsFilter, modeofstudyFilter, levelFilter );
            localStorage.removeItem('searchTerm');
            localStorage.removeItem('modeofstudyFilter');
            localStorage.removeItem('subjectsFilter');
            localStorage.removeItem('levelFilter');
            localStorage.setItem("searchTerm", searchterm);
            $allCheckboxes.attr("disabled", true);
            
            resultScroll(jQuery('.search-results'));
    
        });
    
        //on checking subject checkboxes
        jQuery(".js-subjects").change(function() {
            //check search term
            searchterm = $searchInput.val();
            //get all checked
            subjectsFilter = [];
            jQuery(this).find('input[type="checkbox"]:checked').each(function(i, v) {
                subjectsFilter.push(jQuery(v).val());
            });
            searchResults(searchterm, subjectsFilter, modeofstudyFilter, levelFilter);
            localStorage.setItem("subjectsFilter", subjectsFilter);
            $allCheckboxes.attr("disabled", true);
    
            resultScroll(jQuery('.search-results'));
        
//            if(jQuery(window).width() < 992) {
//                resultScroll();
//            }
        });
    
        //on checking mode checkboxes
        jQuery(".js-moa").change(function() {
            //check search term
            searchterm = $searchInput.val();
            //get all checked
            modeofstudyFilter = [];
            jQuery(this).find('input[type="checkbox"]:checked').each(function(i, v) {
                modeofstudyFilter.push(jQuery(v).val());
            });
            searchResults(searchterm, subjectsFilter, modeofstudyFilter, levelFilter);
            localStorage.setItem("modeofstudyFilter", modeofstudyFilter);
            $allCheckboxes.attr("disabled", true);
            resultScroll(jQuery('.search-results'));
            
        });
    
        //on checking levels checkboxes
        jQuery(".js-level").change(function() {
            //check search term
            searchterm = $searchInput.val();
            //get all checked
            levelFilter = [];
            jQuery(this).find('input[type="checkbox"]:checked').each(function(i, v) {
                levelFilter.push(jQuery(v).val());
            });
            searchResults(searchterm, subjectsFilter, modeofstudyFilter, levelFilter);
            localStorage.setItem("levelFilter", levelFilter);
            $allCheckboxes.attr("disabled", true);
            resultScroll(jQuery('.search-results'));
            
        });
        
        //on clicking subject area block (filter by subject)
        jQuery(".js-subject-block-link").on( "click", function(e) {
            e.preventDefault();
        
            //remove filters for new search
            subjectTid = jQuery(this).attr('data-tid');
            console.log(subjectTid);
            console.log(typeof subjectTid);
            searchterm = "";
            subjectsFilter = [subjectTid];
            modeofstudyFilter = [];
            levelFilter = [];
            searchResults(searchterm, subjectsFilter, modeofstudyFilter, levelFilter );
            localStorage.setItem("subjectsFilter", subjectsFilter);
            
            //mark checkbox as checked
            jQuery('.js-subjects input:checkbox[value="' + subjectTid + '"]').prop('checked', true);
        
        });
    
        // on clear filters
        jQuery(".js-clear-filters").on( "click", function(e) {
            e.preventDefault();
        
            //remove filters for new search
            searchterm = jQuery('#searchterm-cs').val();
            subjectsFilter = [];
            modeofstudyFilter = [];
            levelFilter = [];
            jQuery('input[type="checkbox"]').prop('checked', false).attr("disabled", true);
            searchResults(searchterm, subjectsFilter, modeofstudyFilter, levelFilter );
            localStorage.removeItem('subjectsFilter');
            localStorage.removeItem('modeofstudyFilter');
            localStorage.removeItem('levelFilter');
            resultScroll(jQuery('.search-results'));
            jQuery(this).hide();
        
        });
    
        //on clear search
        jQuery(".js-reset-search").on( "click", function(e) {
            e.preventDefault();

            //remove filters for new search
            jQuery('#searchterm-cs').val("");
            searchterm = "";
            subjectsFilter = [];
            modeofstudyFilter = [];
            levelFilter = [];
            jQuery('input[type="checkbox"]').prop('checked', false).attr("disabled", true);
            //change background
            jQuery('.search-results').removeClass('results-active');
            searchResults(searchterm, subjectsFilter, modeofstudyFilter, levelFilter );
            localStorage.removeItem('searchTerm');
            localStorage.removeItem('subjectsFilter');
            localStorage.removeItem('modeofstudyFilter');
            localStorage.removeItem('levelFilter');

            jQuery(this).hide();

        });
        
    
        function resultScroll(target){
            jQuery('html, body').animate({
                scrollTop: target.offset().top
            }, 700);
        }
    
        //Show initial course list
        jQuery(document).on('click', '.js-all-courses-link', function (e) {
            e.preventDefault();
            
            // hide js results and subject block - show all courses
            jQuery('.js-all-courses').show();
            jQuery(this).attr('aria-expanded','true');
            jQuery('.js-subject-block').hide();
            jQuery('.js-course-intro').html('');
            jQuery('.js-course-count').html('');
            
            //remove search input value and uncheck checkboxes
            jQuery('#searchterm-cs').val("");
            jQuery('input[type="checkbox"]').prop('checked', false);
            
            //change background
            jQuery('.search-results').addClass('results-active');
            
            //hide js results
            resultScroll(jQuery('#all-courses-list'));
            
            //reset search params
            searchterm = "";
            subjectsFilter = [];
            modeofstudyFilter = [];
            levelFilter = [];
            
            //reset js results block markup
            jQuery('.js-courses-title').html("").hide();
            jQuery('.js-results').html("");
    
            //remove local storage
            localStorage.removeItem('searchTerm');
            localStorage.removeItem('subjectsFilter');
            localStorage.removeItem('modeofstudyFilter');
            localStorage.removeItem('levelFilter');
        });
        
    
        // Show/Hide checkbox container
        jQuery(document).on('click', '.js-filters-trigger', function (e) {
            e.preventDefault();
            jQuery(this).parents('.search-filters__col').find('.search-filters__expandable').slideToggle();
            jQuery(this).toggleClass('expanded');
        });
        
        // Hide checkbox container
        jQuery(document).on('click', '.js-filters-close', function (e) {
            e.preventDefault();
            jQuery(this).parents('.search-filters__expandable').slideUp();
            jQuery(this).parents('.search-filters__col').find('.js-filters-trigger').removeClass('expanded');
        });
    
    }, false);
    
</script>

<svg class="hide">
    <defs>
        <symbol id="calendar"><path d="M4.625 9h-1.25A.376.376 0 013 8.625v-1.25C3 7.169 3.169 7 3.375 7h1.25c.206 0 .375.169.375.375v1.25A.376.376 0 014.625 9zM8 8.625v-1.25A.376.376 0 007.625 7h-1.25A.376.376 0 006 7.375v1.25c0 .206.169.375.375.375h1.25A.376.376 0 008 8.625zm3 0v-1.25A.376.376 0 0010.625 7h-1.25A.376.376 0 009 7.375v1.25c0 .206.169.375.375.375h1.25A.376.376 0 0011 8.625zm-3 3v-1.25A.376.376 0 007.625 10h-1.25a.376.376 0 00-.375.375v1.25c0 .206.169.375.375.375h1.25A.376.376 0 008 11.625zm-3 0v-1.25A.376.376 0 004.625 10h-1.25a.376.376 0 00-.375.375v1.25c0 .206.169.375.375.375h1.25A.376.376 0 005 11.625zm6 0v-1.25a.376.376 0 00-.375-.375h-1.25a.376.376 0 00-.375.375v1.25c0 .206.169.375.375.375h1.25a.376.376 0 00.375-.375zM14 3.5v11a1.5 1.5 0 01-1.5 1.5h-11A1.5 1.5 0 010 14.5v-11A1.5 1.5 0 011.5 2H3V.375C3 .169 3.169 0 3.375 0h1.25C4.831 0 5 .169 5 .375V2h4V.375C9 .169 9.169 0 9.375 0h1.25c.206 0 .375.169.375.375V2h1.5A1.5 1.5 0 0114 3.5zm-1.5 10.813V5h-11v9.313c0 .103.084.187.188.187h10.624a.188.188 0 00.188-.188z"></path></symbol>
