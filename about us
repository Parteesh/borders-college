<script>
    document.addEventListener('DOMContentLoaded', function () {
    
        jQuery(document).on('click', '.js-video-control-pause', function (e) {
            e.preventDefault();
            document.getElementById('hero-video').pause();
            jQuery(this).hide();
            jQuery('.js-video-control-play').show();
            
        });
    
        jQuery(document).on('click', '.js-video-control-play', function (e) {
            e.preventDefault();
            document.getElementById('hero-video').play();
            jQuery(this).hide();
            jQuery('.js-video-control-pause').show();
        });
       
        
    })
</script>





