    window.addEventListener('DOMContentLoaded', function () {
        jQuery(document).on('click','.js-video-1356', function (e) {
            e.preventDefault();
            jQuery(this).parent('.para-cta-panel__iframe-container').addClass('video-played');
            var videoSrc = jQuery(this).siblings('iframe').attr('data-src');
            jQuery(this).siblings('iframe').attr('src', videoSrc+'?autoplay=1&rel=0').removeAttr('tabindex');
        });
        
        var highlightText = 'Free';
    
