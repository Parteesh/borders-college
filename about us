        document.addEventListener('DOMContentLoaded', function () {
            if (window.matchMedia("(min-width: 768px)").matches && jQuery('.node-homepage').length) {
                var cardHeight = jQuery(".js-row-height-1101 .para-page-panel__card").height();
                var bottomPadding = 64;
                var imgHeight = jQuery(".js-row-height-1101 .para-page-panel__img").height();
                var backHeight = (cardHeight + bottomPadding) - (imgHeight + 16) + "px";
                jQuery('.para-page-panel__back--1101').css('height', backHeight);
            }
        });
    </script>

